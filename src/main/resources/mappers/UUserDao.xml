<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bx.gis.mapper.UUserDao">

    <select id="selectAllByName" resultType="com.bx.gis.entity.UUser">
      SELECT
        *
      FROM
        u_user
      WHERE
        nickname = #{username}
    </select>
</mapper>